{
  "name": "angular2-config-generator",
  "version": "1.0.5",
  "description": "An opinionated configuration generator for your Angular 2 environment. (Webpack 2, Karma and Jasmine)",
  "main": "index.js",
  "scripts": {
    "ng-start": "node ./bin/ng-start.js --aot",
    "ng-build": "node ./bin/ng-build.js --aot",
    "ng-deploy": "node ./bin/ng-deploy.js",
    "ng-test": "node ./bin/ng-test.js",
    "ng-test-watch": "node ./bin/ng-test.js --watch",
    "build": "webpack",
    "deploy": "node ./bin/ng-deploy.js",
    "deploy-npm": "np --skip-cleanup",
    "lint": "tslint \"src/**/*.ts\"",
    "start": "webpack-dev-server",
    "start-prod": "http-server ./dist -p 3000",
    "test": "karma start",
    "test-watch": "karma start"
  },
  "bin": {
    "ng-start": "./bin/ng-start.js",
    "ng-build": "./bin/ng-build.js",
    "ng-deploy": "./bin/ng-deploy.js",
    "ng-test": "./bin/ng-test.js"
  },
  "dependencies": {
    "@angular/compiler-cli": "~2.1.2",
    "@angular/platform-server": "~2.1.2",
    "@types/core-js": "~0.9.34",
    "@types/jasmine": "~2.5.37",
    "@types/node": "~6.0.46",
    "angular2-router-loader": "~0.3.3",
    "angular2-template-loader": "~0.6.0",
    "awesome-typescript-loader": "~2.2.4",
    "chokidar": "~1.6.1",
    "codelyzer": "~1.0.0-beta.3",
    "copy-webpack-plugin": "~4.0.0",
    "css-loader": "~0.25.0",
    "extract-text-webpack-plugin": "~2.0.0-beta.4",
    "file-loader": "~0.9.0",
    "gh-pages": "~0.11.0",
    "html-loader": "~0.4.4",
    "jasmine-core": "~2.5.2",
    "karma": "~1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "~1.0.2",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.8.0",
    "minimist": "~1.2.0",
    "source-map-loader": "~0.1.5",
    "style-loader": "~0.13.1",
    "to-string-loader": "~1.1.5",
    "ts-helpers": "~1.1.2",
    "tslint": "~3.15.1",
    "tslint-loader": "~2.1.5",
    "typescript": "~2.0.9",
    "url-loader": "~0.5.7",
    "webpack": "~2.1.0-beta.25",
    "webpack-dev-server": "~2.1.0-beta.10"
  },
  "devDependencies": {
    "angular2-bundle": "2.1.2",
    "bootstrap": "3.3.7",
    "http-server": "0.9.0",
    "np": "2.10.0"
  },
  "peerDependencies": {
    "@angular/compiler": "2.1.2",
    "@angular/core": "2.1.2"
  }
  "engines": {
    "node": ">=6"
  },
  "author": {
    "name": "blacksonic",
    "email": "soos.gabor86@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blacksonic/angular2-config-generator.git"
  },
  "keywords": [
    "angular2",
    "webpack",
    "karma",
    "jasmine"
  ],
  "license": "ISC"
}
