{
  "name": "ngx-webpack",
  "version": "3.0.0",
  "description": "An opinionated configuration generator for your Angular 2 environment. (Webpack 2, Karma and Jasmine)",
  "main": "index.js",
  "scripts": {
    "ng-start": "node ./bin/ng-start.js --aot",
    "ng-build": "node ./bin/ng-build.js --aot",
    "ng-deploy": "node ./bin/ng-deploy.js",
    "ng-lint": "node ./bin/ng-lint.js",
    "ng-test": "node ./bin/ng-test.js",
    "ng-test-watch": "node ./bin/ng-test.js --watch",
    "ng-init": "node ./bin/ng-init.js",
    "build": "webpack",
    "deploy": "node ./bin/ng-deploy.js",
    "deploy-npm": "np --skip-cleanup",
    "lint": "tslint \"src/**/*.ts\"",
    "start": "webpack-dev-server",
    "start-prod": "http-server ./dist -p 3000",
    "test": "karma start",
    "test-watch": "karma start"
  },
  "bin": {
    "ng-start": "./bin/ng-start.js",
    "ng-build": "./bin/ng-build.js",
    "ng-deploy": "./bin/ng-deploy.js",
    "ng-test": "./bin/ng-test.js",
    "ng-init": "./bin/ng-init.js",
    "ng-lint": "./bin/ng-lint.js"
  },
  "dependencies": {
    "@ngtools/webpack": "1.2.1",
    "@types/jasmine": "~2.5.40",
    "@types/node": "~6.0.56",
    "angular2-router-loader": "~0.3.4",
    "angular2-template-loader": "~0.6.0",
    "apply-loader": "~0.1.0",
    "awesome-typescript-loader": "~3.0.0-beta.17",
    "codelyzer": "~2.0.0-beta.4",
    "copy-webpack-plugin": "~4.0.1",
    "css-loader": "~0.26.1",
    "extract-text-webpack-plugin": "~2.0.0-beta.4",
    "file-loader": "~0.9.0",
    "fs-extra": "~1.0.0",
    "gh-pages": "~0.12.0",
    "html-loader": "~0.4.4",
    "jasmine-core": "~2.5.2",
    "karma": "~1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-phantomjs-launcher": "~1.0.2",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.8.1",
    "minimist": "~1.2.0",
    "style-loader": "~0.13.1",
    "to-string-loader": "~1.1.5",
    "ts-helpers": "~1.1.2",
    "tslint": "~4.2.0",
    "tslint-loader": "~3.3.0",
    "typescript": "~2.0.10",
    "url-loader": "~0.5.7",
    "webpack": "~2.1.0-beta.28",
    "webpack-dev-server": "~2.1.0-beta.12"
  },
  "devDependencies": {
    "@angular/common": "2.4.1",
    "@angular/compiler": "2.4.1",
    "@angular/compiler-cli": "2.4.1",
    "@angular/core": "2.4.1",
    "@angular/forms": "2.4.1",
    "@angular/http": "2.4.1",
    "@angular/platform-browser": "2.4.1",
    "@angular/platform-browser-dynamic": "2.4.1",
    "@angular/platform-server": "2.4.1",
    "@angular/router": "3.4.1",
    "core-js": "2.4.1",
    "http-server": "0.9.0",
    "ng2-translate": "~5.0.0",
    "node-sass": "^4.1.1",
    "np": "2.12.0",
    "rxjs": "5.0.2",
    "sass-loader": "^4.1.1",
    "zone.js": "0.7.4"
  },
  "peerDependencies": {
    "@angular/compiler": "2.4.1",
    "@angular/core": "2.4.1"
  },
  "engines": {
    "node": ">=6"
  },
  "author": {
    "name": "blacksonic",
    "email": "soos.gabor86@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blacksonic/ngx-webpack.git"
  },
  "keywords": [
    "angular2",
    "webpack",
    "karma",
    "jasmine"
  ],
  "license": "ISC"
}
